{
    "name": "zfcampus/zf-api-problem",
    "description": "ZF2 Module providing API-Problem assets and rendering",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rest",
        "api-problem"
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com" },
        { "type": "composer", "url": "http://packages-zfcampus.rhcloud.com" }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-eventmanager": "dev-develop",
        "zendframework/zend-json": "dev-develop",
        "zendframework/zend-mvc": "dev-develop",
        "zendframework/zend-view": "dev-develop"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "zendframework/zend-console": "dev-develop",
        "zendframework/zend-http": "dev-develop",
        "zendframework/zend-loader": "dev-develop"
    },
    "autoload": {
        "psr-0": {
            "ZF\\ApiProblem\\": "src/",
            "ZFTest\\": "test/"
        }
    }
}
